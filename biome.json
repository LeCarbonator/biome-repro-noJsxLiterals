{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"files": {
		"includes": [
			"src/**/*.jsx"
		]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": false,
			"nursery": {
				"noJsxLiterals": {
					"level": "error",
					"options": {
						"allowedStrings": [
							"allowed"
						]
					}
				}
			}
		}
	}
}